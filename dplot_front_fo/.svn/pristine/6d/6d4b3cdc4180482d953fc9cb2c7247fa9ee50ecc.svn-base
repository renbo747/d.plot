.dp-app-error {
  height: calc((var(--vh, 1vh) * 100) - 100px);
  //header
  .error-header {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100px;
    background-color: rgba(0,0,0,.8);
  }
  .error-header-logo {
    position: relative;
    width: 100%;
  }
  .error-header-logo__figure {
    position: absolute;
    top: 56px;
    left: 0;
  }
  // body 시작
  .dp-app-error-wrap {
    padding-top: 280px;
  }
  .dp-app-error__title {
    padding-top: 30px;
    padding-bottom: 30px;
  }
  .dp-app-error__title__p {
    font-size: 24px;
    letter-spacing: -.96px;
    font-weight: 400;
  }
  .dp-app-error__text__wrap {
    padding-bottom: 130px;
  }
  .dp-app-error-wrap {
    text-align: center;
  }
  .dp-app-error__text {
    font-size: 18px;
    letter-spacing: -.72px;
  }
  .btn-group {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    .dp-btn {
      max-width: 320px;
    }
  }
  .error-button__text {
    font-size: 16px;
    letter-spacing: -.64px;
  }
}

