.mypage-previous-order-pc {
  min-height: calc((var(--vh, 1vh) * 100) - 110px);
  .previous-order__title__area {
    h1{
      font-size: 26px;
      letter-spacing: -1.04px;
      font-weight: 700;
    }
  }
  // 이전구매내역 있을 때
  .previous-order-hr {
    background: $gray-100;
    height: 6px;
    margin: 80px 0;
  }
  .dp-hr {
    &.h1 {
      margin: 30px 0;
    }
  }
  .order-list__wrap {
    &:first-child {
      margin-top: 66px;
    }
    .order-date__area {
      display: flex;
      margin-bottom: -10px;
      .date__P {
        font-size: 18px;
        letter-spacing: -0.54px;
        font-weight: 500;
        color: $gray-800;
      }
      .dp-bar {
        margin-top: 6px;
        margin-left: 15px;
        margin-right: 10px;
      }
      .order-number__p {
        font-size: 18px;
        letter-spacing: -.54px;
        font-weight: 500;
        color: $gray-700;
      }
    }
    .product-info__area {
      .product-info__left {
        width: calc(100% - 176px);
        .product-title {
          width: 410px;
          font-size: 16px;
          letter-spacing: -.64px;
          color: $gray-800;
          overflow: hidden;
          text-overflow: ellipsis;
          display: -webkit-box;
          -webkit-line-clamp: 1;
          -webkit-box-orient: vertical;
        }
      }
      .product-into__right {
        width: 176px;
        justify-content: space-between;
        display: flex;
        .product-conunt {
          font-size: 16px;
          letter-spacing: -.64px;
          color: $gray-700;
        }
        .product-price__area {
          display: flex;
          .product-price {
            font-size: 18px;
            letter-spacing: -.54px;
            font-weight: 600;
          }
          .price-unit {
            margin-top: 2px;
            margin-left: 2px;
            font-size: 14px;
            letter-spacing: -.56px;
          }
        }
      }
    }
  }
  // 이전구매내역 없을 때
  .empty__area {
    padding-top: 280px;
    .empty__body {
      .empty-icon-area {
        width: 100px;
        height: 100px;
        background: $gray-100;
        border-radius: 50%;
      }
      .empty-text__area {
        padding-top: 30px;
      }
      .empty-text__p {
        font-size: 18px;
        letter-spacing: -0.72px;
        color: $gray-700;
      }
    }
  }
}
