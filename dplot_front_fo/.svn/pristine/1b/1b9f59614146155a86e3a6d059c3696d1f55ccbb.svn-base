.cs-faq {
  .cursor {
    cursor: pointer;
  }
  .accordion__header .header__text {
    padding-left: 26px;
  }
  .accordion__header .header__text:before {
    top: -1px;
    font-size: 20px;
  }
  .cs-section__title {
    font-size: 26px;
    letter-spacing: -1.04px;
  }
  // 자주묻는 질문 필터
  .question-filter {
    .question__ul {
      display: flex;
      flex-wrap: wrap;
      border-top: 1px solid $border-color;
      border-left: 1px solid $border-color;
      li {
        width: 25%;
        height: 50px;
        border-right: 1px solid $border-color;
        border-bottom: 1px solid $border-color;
        padding:0;
      }

    }
    .question__label {
      display: block;
      width: 100%;
      height: 100%;
      cursor: pointer;

      .label__item {
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
        height: 48px;
        padding: 0;
        font-size: 16px;
        letter-spacing: -.48px;
        font-weight: 400;
        color: $gray-600;

      }

      input:checked + .label__item {
        position: relative;
        top: -1px;
        left: -1px;
        color: $white;
        font-size: 18px;
        font-weight: 500;
        background-color: $gray-800;
        width: calc(100% + 2px);
        height: calc(100% + 2px);
      }
    }
  }
  // 자주하는 질문 회원/멤버 카테고리-question-category ( cs index scss 참고 )
  .question-category {
    padding-top: 80px;
    .question-category-header {
      padding-bottom: 50px;
    }
    .question-category__title {
      font-size: 22px;
      letter-spacing: -.88px;
    }
    .question-category-body {
      padding-bottom: 30px;
    }
    .accordion__body {
      font-size: 18px;
      letter-spacing: -.72px;
    }
  }
  .question-category-pagination {
    align-items: center;
    justify-content: center;
  }
  .header__text {
    line-height: 1;
    span {
      color: $primary;
    }
  }
  .cs-faq__btn {
    align-items: center;
    justify-content: space-between;
  }
  .cs-faq__btn_inquiry-item {
    padding: 0 120px;
  }
  .cs-faq__total {
    text-align: center;
    padding-bottom: 80px;
  }
  .cs-faq__total-text {
    font-size: 18px;
    letter-spacing: -.54px;
  }
  .cs-faq__result {
    padding-top: 30px;
  }
  .cs-faq-find-result__empty {
    padding: 80px 0;
  }
  .cs-faq__total-empty {
    text-align: center;
  }
  .cs-faq__total-p {
    font-size: 18px;
    letter-spacing: -.72px;
  }

  .cs-faq-find__fail {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 94px;
    height: 94px;
    border-radius: 50%;
    background-color: $gray-100;
  }
  .icon-not-found {
    background-image: url("~@/assets/common/icon/icon-not-found-black-64px.svg");
  }
  //common
  .mb-20 {
    margin-bottom: 20px;
  }
  .pb-80 {
    padding-bottom: 80px;
  }
}
